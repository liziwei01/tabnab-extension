{
    "manifest_version": 3,
    "name": "tabnab-extension",
    "description": "a tabnab extension",
    "version": "0.0.1",
	"permissions": [
        "tabs",
		"tabGroups",
        "activeTab",
        "notifications",
        "storage"
    ],
	"host_permissions": [
		"*://*/*"
	],
	"background": {
        "scripts": []
    },
	"action": {
		"default_popup": "./src/template/popup.html"
	},
	"icons": {
		"16": "./src/images/icon-16.png",
		"32": "./src/images/icon-32.png",
		"48": "./src/images/icon-48.png",
		"128": "./src/images/icon-128.png"
	}
}